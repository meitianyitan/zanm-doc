{
  "name": "zanm-doc",
  "version": "0.0.4",
  "description": "zanm document site template",
  "keywords": [
    "doc",
    "vue",
    "meitianyitan"
  ],
  "author": "Weich",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meitianyitan/zanm-doc.git"
  },
  "bugs": {
    "url": "https://github.com/meitianyitan/zanm-doc/issues"
  },
  "homepage": "https://github.com/meitianyitan/zanm-doc#readme",
  "main": "./dist/index.js",
  "bin": {
    "zanm-doc": "./cli/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config ./build/webpack.dev.conf.js",
    "build": "webpack --hide-modules --progress --config ./build/webpack.prod.conf.js",
    "release": "npm run lint && npm run build && npm publish",
    "lint": "eslint --fix --ext .js,.vue \"**/*.{js,vue}\" \"*.{js,vue}\" && stylelint \"**/*.{vue,css,scss}\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "git add"
    ],
    "*.{vue,css}": [
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.19.0",
    "decamelize": "^2.0.0",
    "fs-extra": "^7.0.0",
    "nprogress": "^0.2.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "add": "^2.0.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.1",
    "eslint-plugin-vue": "^4.7.1",
    "eslint-plugin-vue-libs": "^3.0.0",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "yarn": "^1.10.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
